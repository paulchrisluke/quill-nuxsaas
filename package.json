{
  "name": "quillio",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "test": "vitest run --silent=false",
    "test:chat": "vitest run tests/integration/chat.test.ts tests/e2e/chat-codex.test.ts",
    "test:chat-manual": "npx tsx tests/manual/chat-runner.ts",
    "test:chat-custom": "npx tsx tests/manual/chat-runner.ts custom",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "auth:schema": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/database/schema/auth.ts -y",
    "db:generate": "drizzle-kit generate --config ./server/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts",
    "build": "nuxt build",
    "serve": "node .output/server/index.mjs",
    "preview:cf": "wrangler dev --x-remote-bindings",
    "deploy": "wrangler deploy",
    "prepare": "husky"
  },
  "dependencies": {
    "@better-auth/stripe": "^1.4.1",
    "@nuxt/ui": "^4.2.1",
    "@nuxthub/core": "^0.9.1",
    "@nuxtjs/i18n": "^9.5.6",
    "@nuxtjs/mdc": "^0.19.1",
    "@nuxtjs/seo": "^3.2.2",
    "@react-email/render": "^2.0.0",
    "@vueuse/core": "13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "aws4fetch": "^1.0.20",
    "better-auth": "^1.4.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "0.44.5",
    "echarts": "^5.6.0",
    "ioredis": "^5.8.2",
    "nuxt": "^4.2.1",
    "nuxt-echarts": "^0.4.0",
    "ofetch": "^1.5.1",
    "pg": "^8.16.3",
    "resend": "^6.5.2",
    "sortablejs": "^1.15.6",
    "stripe": "^20.0.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.24",
    "vue-component-type-helpers": "3.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@better-auth/cli": "^1.4.1",
    "@iconify-json/lucide": "^1.2.74",
    "@iconify-json/simple-icons": "^1.2.59",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/test-utils": "^3.20.1",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "playwright-core": "^1.56.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.50.0"
  }
}
