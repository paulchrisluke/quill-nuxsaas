{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    ".nuxt/**/*.d.ts",
    "server/types/**/*.d.ts",
    "nuxt.config.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue"
  ]
}
